<variant
    name="civRelease"
    package="com.atakmap.android.geospy.plugin"
    minSdkVersion="26"
    targetSdkVersion="33"
    shrinking="true"
    mergedManifest="build/intermediates/merged_manifest/civRelease/AndroidManifest.xml"
    manifestMergeReport="build/outputs/logs/manifest-merger-civ-release-report.txt"
    proguardFiles="proguard-gradle.txt"
    partialResultsDir="build/intermediates/lint_vital_partial_results/civRelease/out">
  <buildFeatures
      namespacing="REQUIRED"/>
  <sourceProviders>
    <sourceProvider
        manifest="src/main/AndroidManifest.xml"
        javaDirectories="src/main/java:src/main/kotlin"
        resDirectories="src/main/res"
        assetsDirectories="src/main/assets"/>
    <sourceProvider
        manifest="src/civ/AndroidManifest.xml"
        javaDirectories="src/civ/java:src/civ/kotlin"
        resDirectories="src/civ/res"
        assetsDirectories="src/civ/assets"/>
    <sourceProvider
        manifest="build-types/release/AndroidManifest.xml"
        javaDirectories="build-types/release/java:build-types/release/kotlin"
        resDirectories="build-types/release/res"
        assetsDirectories="build-types/release/assets"/>
    <sourceProvider
        manifest="src/civRelease/AndroidManifest.xml"
        javaDirectories="src/civRelease/java:src/civRelease/kotlin"
        resDirectories="src/civRelease/res"
        assetsDirectories="src/civRelease/assets"/>
  </sourceProviders>
  <testSourceProviders>
  </testSourceProviders>
  <testFixturesSourceProviders>
  </testFixturesSourceProviders>
  <manifestPlaceholders>
    <placeholder
        name="atakApiVersion"
        value="com.atakmap.app@4.10.0.CIV" />
  </manifestPlaceholders>
  <mainArtifact
      classOutputs="build/intermediates/javac/civRelease/classes:build/intermediates/compile_and_runtime_not_namespaced_r_class_jar/civRelease/R.jar"
      applicationId="com.atakmap.android.geospy.plugin"
      generatedSourceFolders="build/generated/ap_generated_sources/civRelease/out:build/generated/aidl_source_output_dir/civRelease/out:build/generated/source/buildConfig/civ/release:build/generated/renderscript_source_output_dir/civRelease/out"
      generatedResourceFolders="build/generated/res/rs/civ/release:build/generated/res/resValues/civ/release">
  </mainArtifact>
</variant>
